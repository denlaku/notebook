    "from pyspark import SparkConf, SparkContext\n",
    "conf = SparkConf().setMaster(\"local\").setAppName(\"First App\")\n",
    "sc = SparkContext.getOrCreate(conf)\n",
    "\n",
    "names = [\"张无忌\", \"赵敏\", \"周芷若\"]\n",
    "\n",
    "rdd = sc.parallelize(names).map(lambda name: 'hello: ' + name).collect()\n",
    "rdd"
   ]
  }
